{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "INT3RCEPTOR",
  "version": "3.0.0-beta",
  "identifier": "com.s1bgroup.int3rceptor",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "INT3RCEPTOR - Advanced HTTP/HTTPS Proxy",
        "width": 1600,
        "height": 900,
        "minWidth": 1200,
        "minHeight": 700,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "transparent": false,
        "alwaysOnTop": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "publisher": "S1BGr0uP",
    "copyright": "Copyright Â© 2025 S1BGr0uP",
    "category": "DeveloperTool",
    "shortDescription": "Advanced HTTP/HTTPS intercepting proxy",
    "longDescription": "INT3RCEPTOR is a powerful HTTP/HTTPS intercepting proxy built with Rust and WebAssembly plugins for security testing and analysis."
  }
}
